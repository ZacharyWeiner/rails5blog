<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= @page_title %></title>
    <%= csrf_meta_tags %>
    <meta name="keywords" content="<%= @seo_keywords%>"></meta>
    <%= stylesheet_link_tag    'portfolios', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <!-- Begin Nav --> 
    <%= render 'shared/portfolio_nav'%>
    <!-- End Nav --> 
    <!-- Begin MasHead -->
    <%= render 'shared/portfolio_masthead'%>
    <!-- End MasHead --> 
    <%= yield %>
    <!--  Begin footer --> 
     <%= render 'shared/portfolio_footer'%>
    <!-- End footer -->
    <%= source_helper %>
  </body>
</html>
