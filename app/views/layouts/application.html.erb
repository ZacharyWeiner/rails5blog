<!DOCTYPE html>
<html>
  <head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= @page_title %></title>
    <%= csrf_meta_tags %>
    <meta name="keywords" content="<%= @seo_keywords%>"></meta>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="site-wrapper">
    <div class="site-wrapper-inner">
      <div class="cover-container">
        <!-- <div id='background'> </div> -->
        <%= video_tag 'https://www.videvo.net/app/stream.php?id=2747', id: 'background',  autoplay: true, loop: true, muted: true %>
        <%= render "shared/application_nav" %>
        
        <%= yield %>
        <br>
        <%= render '/shared/application_footer'%>
        <%= render 'shared/alerts' %>
        <%= source_helper %>
      </div>
    </div>
  </div>
  </body>
</html>
